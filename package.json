{
  "name": "styleguide-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate KSS node styleguide implementation for projects.",
  "main": "index.js",
  "scripts": {
    "generate-styleguide": "./node_modules/.bin/kss-node scss --template styleguide/caxy-template --css ../css/main.css --custom devnotes --custom hidemarkup",
    "generate-styleguide-verbose": "./node_modules/.bin/kss-node scss --template styleguide/caxy-template --css ../css/main.css --verbose --custom devnotes --custom hidemarkup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caxy/styleguide-boilerplate.git"
  },
  "keywords": [
    "kss",
    "node",
    "css",
    "scss",
    "styleguide"
  ],
  "author": "Beck Kramer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caxy/styleguide-boilerplate/issues"
  },
  "homepage": "https://github.com/caxy/styleguide-boilerplate#readme",
  "devDependencies": {
    "kss": "^2.4.0",
    "node-sass": "^3.4.2"
  }
}
